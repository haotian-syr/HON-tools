<Group labelreference="L109" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">bin := proc(A, B)
       local ii, bs;
       bs := []:
       

       for ii from 1 to card(A) do
           if member(A[ii], B) then
              bs := append(bs, 1)
           else
              bs := append(bs, 0):
           fi:
       od:
       bs:
end proc:
</Text-field>
</Input>
